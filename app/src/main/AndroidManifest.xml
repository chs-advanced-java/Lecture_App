<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ajlyfe.lectureapp">

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".Activity.UploadActivity"
            android:label="Upload" />

        <activity android:name=".Activity.TeacherClassMainScreen"
            android:label="My Classes" />

        <activity android:name=".Activity.TeacherMainScreen"
            android:label="RecoLecture">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Activity.TeacherClassViewScreen"
            android:label="My Classes" />

    </application>

</manifest>